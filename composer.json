{
    "name": "my/design-pattern",
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "*",
        "friendsofphp/php-cs-fixer": "^2.16"
    },

    "config": {
        "platform": {
            "php": "7.3.11"
        }
    },

    "autoload": {
        "psr-4": {
